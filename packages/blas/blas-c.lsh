;;*           triangular band matrix from conventional full matrix storage
;;*           to band storage:
;;*
;;*                 DO 20, J = 1, N
;;*                    M = K + 1 - J
;;*                    DO 10, I = MAX( 1, J - K ), J
;;*                       A( M + I, J ) = matrix( I, J )
;;*              10    CONTINUE
