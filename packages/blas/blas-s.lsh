;;*
;;*
;;*     .. External Functions ..
;;*  =====================================================================

(de ssyrk (uplo  trans  n  k  alpha  a  lda  beta  c  ldc  )
	((-str-) uplo )
	((-str-) trans )
	((-idx0- (-int-)) n )
	((-idx0- (-int-)) k )
	((-idx0- (-float-)) alpha )
	((-idx2- (-float-)) a )
