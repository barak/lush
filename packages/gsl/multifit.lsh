(de gsl_multifit_test_delta (    dx  x  epsabs  epsrel )
    ((-gptr- "const gsl_vector *") dx) 
    ((-gptr- "const gsl_vector *") x) 
    ((-double-) epsabs) 
    ((-double-) epsrel) 
    (int #{ gsl_multifit_test_delta (  $dx ,$x ,$epsabs ,$epsrel) #} )
