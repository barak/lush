(de gsl_multifit_fdfsolver_set (    s  fdf  x )
    ((-gptr- "gsl_multifit_fdfsolver *") s) 
    ((-gptr- "gsl_multifit_function_fdf *") fdf) 
    ((-gptr- "gsl_vector *") x) 
