(libload "gsl/gsl-config")



#? *** sum

;; dummy function that adds GSL C header files in C file
(eval `(de dummy_sum () ,gsl-cpheader-cmd ()))

#? * (dummy_sum)
#? * (gsl_sum_levin_u_alloc <n>)
