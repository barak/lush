)


(de gsl_sum_levin_utrunc_minmax (     array  n  min_terms  max_terms  w   sum_accel   abserr_trunc )
    ((-gptr- "const double *") array) 
    ((-int-) n)                     ;; size_t -> int
    ((-int-) min_terms)                     ;; size_t -> int
    ((-int-) max_terms)                     ;; size_t -> int
    ((-gptr- "gsl_sum_levin_utrunc_workspace *") w) 
