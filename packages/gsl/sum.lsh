(dhc-make ()
  dummy_sum
  gsl_sum_levin_u_alloc
  gsl_sum_levin_u_free
  gsl_sum_levin_u_accel
  gsl_sum_levin_u_minmax
  gsl_sum_levin_u_step
  gsl_sum_levin_utrunc_alloc
