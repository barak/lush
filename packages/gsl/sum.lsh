    #{ gsl_sum_levin_utrunc_free (  $w) #} 
    ()
)


(de gsl_sum_levin_utrunc_accel (     array  n  w   sum_accel   abserr_trunc )
    ((-gptr- "const double *") array) 
    ((-int-) n)                     ;; size_t -> int
    ((-gptr- "gsl_sum_levin_utrunc_workspace *") w) 
    ((-gptr- "double *") sum_accel) 
    ((-gptr- "double *") abserr_trunc) 
