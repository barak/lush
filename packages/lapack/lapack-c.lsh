	((-idx0- (-int-)) n )
	((-idx0- (-int-)) nb )
	((-idx0- (-int-)) kb )
	((-idx3- (-float-)) a )
	((-idx0- (-int-)) lda )
	((-idx1- (-int-)) ipiv )
	((-idx3- (-float-)) w )
	((-idx0- (-int-)) ldw )
	((-idx0- (-int-)) info )
	#{ clasyf_($uplo->data, IDX_PTR($n,int), IDX_PTR($nb,int), IDX_PTR($kb,int), IDX_PTR($a,float), IDX_PTR($lda,int), IDX_PTR($ipiv,int), IDX_PTR($w,float), IDX_PTR($ldw,int), IDX_PTR($info,int)) #} ())
